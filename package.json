    "@mui/icons-material": "^5.3.1",
    "@mui/material": "latest",
    "@openzeppelin/contracts": "latest",
    "@polkadot/extension-dapp": "^0.40.3",
    "@polkadot/rpc-provider": "^6.5.2",
    "@polkadot/ui-keyring": "^0.86.2",
    "@reef-defi/evm-provider": "^1.0.2",
    "bn": "^1.0.5",
    "copy-to-clipboard": "^3.3.1",
    "ethers": "^5.5.4",
    "googleapis": "^95.0.0",
    "ipfs-http-client": "^55.0.0",
